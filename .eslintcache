son from being added to favourites again.\r\n    return state.favourites.some((person) => person.id === currentPersonID);\r\n  };\r\n\r\n  const editClickHandler = (e) => {\r\n    e.stopPropagation();\r\n    const mql = window.matchMedia('(max-width: 768px)');\r\n    if (mql.matches) {\r\n      setShowBackground(true);\r\n    }\r\n    setShowEditPersonUI(true);\r\n    setTimeout(() => setCurrentPersonID(null), 0);\r\n  };\r\n\r\n  const addToFavoritesHandler = () => {\r\n    dispatch({\r\n      type: 'ADD_FAVOURITE',\r\n      payload: [...state.favourites, person],\r\n    });\r\n    putItemToIDB(person, 'userDatabase', '1', 'favourites');\r\n    setCurrentPersonID(null);\r\n  };\r\n\r\n  const removeFromFavouritesHandler = () => {\r\n    const newFavourites = state.favourites.filter(\r\n      (person) => person.id !== currentPersonID\r\n    );\r\n    dispatch({\r\n      type: 'REMOVE_FAVOURITE',\r\n      payload: newFavourites,\r\n    });\r\n    removeDataFromIDBStore('userDatabase', '1', 'favourites', currentPersonID);\r\n    setCurrentPersonID(null);\r\n  };\r\n\r\n  const setHandlerFunction = (e) => {\r\n    e.stopPropagation();\r\n    setShowBackground(false);\r\n    return isPersonInFavourites()\r\n      ? removeFromFavouritesHandler\r\n      : addToFavoritesHandler;\r\n  };\r\n\r\n  const setText = () => {\r\n    return isPersonInFavourites()\r\n      ? 'Remove from favourites'\r\n      : 'Add to favourites';\r\n  };\r\n\r\n  const handleMouseOver = (e) => {\r\n    const target = e.target.closest('div');\r\n    console.log('mouse over');\r\n    const handleMouseOut = () => {\r\n      console.log(\"mouse out\")\r\n      setCurrentPersonID(null);\r\n      target.removeEventListener('mouseleave', handleMouseOut);\r\n    };\r\n    target.addEventListener('mouseleave', handleMouseOut);\r\n  };\r\n\r\n  return (\r\n    <div className=\"person-options-container\" onMouseEnter={handleMouseOver}>\r\n      <p className=\"person-options-container__name\">Person: {person.name}</p>\r\n      <ul className=\"person-options-list\">\r\n        <li className=\"person-options-list__item person-options-list__item--triangled\">\r\n          <button\r\n            className=\"person-options-list__btn\"\r\n            onClick={(e) => setHandlerFunction(e)()}\r\n          >\r\n            {setText()}\r\n          </button>\r\n        </li>\r\n        {/* <hr className=\"person-options-list__line\" /> */}\r\n        <li className=\"person-options-list__item person-options-list__item--edit-btn\">\r\n          <button\r\n            className=\"person-options-list__btn\"\r\n            onClick={(e) => editClickHandler(e)}\r\n          >\r\n            Edit\r\n          </button>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default PersonOptions;\r\n","C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\helper\\findPersonByID.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\helper\\removePersonFromFavourites.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\helper\\FilterPeopleByName.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\context\\AppContext\\AppContext.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\components\\DarkBackground\\DarkBackground.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\components\\AddPersonUIControls\\AddPersonUIControls.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\birthday-reminder-app\\src\\components\\AddPersonUIInfo\\AddPersonUIInfo.js",[],"C:\\Users\\Muse\\Desktop\\Yazılım\\Web Tasarım\\Web Projects\\Birthday Reminder App\\b